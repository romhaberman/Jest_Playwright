{
	"name": "jsaqa_3",
	"version": "1.0.0",
	"description": "jest test",
	"main": "index.js",
	"scripts": {
		"test": "jest",
		"coverage": "jest --collectCoverage"
	},
	"collectCoverage": {
		"collectCoverageFrom": [
			"**/*.{js,jsx}",
			"!**/node_modules/**",
			"!**/coverage/**"
		],
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100
			}
		}
	},
	"keywords": [
		"jest",
		"test",
		"jsaqa"
	],
	"author": "Roman Khabarov",
	"license": "ISC",
	"devDependencies": {
		"jest": "^27.5.1"
	}
}